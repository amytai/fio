[global]
name=fio-rand-RW
#filename=/mnt/docker_nvme/backup.img
filename=/dev/nvme0n1
blocksize=4K
rw=randread
direct=1
time_based=1
runtime=30
group_reporting

[file1]
ioengine=io_uring
hipri=1
offset=10G
size=400G
;barrier=1
numjobs=1
iodepth=16
iodepth_batch_submit=16
iodepth_batch_complete_min=1
iodepth_batch_complete_max=16

[file2]
new_group
ioengine=pvsync2
hipri=1
offset=10G
size=8G
numjobs=1
